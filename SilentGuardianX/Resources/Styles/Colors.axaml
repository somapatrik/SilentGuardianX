<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Colors -->
    <Color x:Key="Primary">#0b5ed7</Color>
    <Color x:Key="Secondary">#DFD8F7</Color>
    <Color x:Key="Tertiary">#2B0B98</Color>
    <Color x:Key="White">White</Color>
    <Color x:Key="Black">Black</Color>
    <Color x:Key="Gray100">#E1E1E1</Color>
    <Color x:Key="Gray200">#C8C8C8</Color>
    <Color x:Key="Gray300">#ACACAC</Color>
    <Color x:Key="Gray400">#919191</Color>
    <Color x:Key="Gray500">#6E6E6E</Color>
    <Color x:Key="Gray600">#404040</Color>
    <Color x:Key="Gray900">#212121</Color>
    <Color x:Key="Gray950">#141414</Color>
    <Color x:Key="Yellow100Accent">#F7B548</Color>
    <Color x:Key="Yellow200Accent">#FFD590</Color>
    <Color x:Key="Yellow300Accent">#FFE5B9</Color>
    <Color x:Key="Cyan100Accent">#28C2D1</Color>
    <Color x:Key="Cyan200Accent">#7BDDEF</Color>
    <Color x:Key="Cyan300Accent">#C3F2F4</Color>
    <Color x:Key="Blue100Accent">#3E8EED</Color>
    <Color x:Key="Blue200Accent">#72ACF1</Color>
    <Color x:Key="Blue300Accent">#A7CBF6</Color>
    <Color x:Key="GridBackground">#4F000000</Color>
    <Color x:Key="NexenColor">#9d1c9d</Color>
    <Color x:Key="SomaDarkDark">#22272e</Color>
    <Color x:Key="SomaDarkLight">#434c57</Color>
    <Color x:Key="GithubDark">#24292f</Color>
    <Color x:Key="GithubLight">#f6f8fa</Color>
    <Color x:Key="GithubRed">#cf222e</Color>
    <Color x:Key="GithubGreen">#2da44e</Color>
    <Color x:Key="BootstrapWarningBg">#ffc107</Color>
    <Color x:Key="LazyColor">#187ccf</Color>
    <Color x:Key="GrayBackground">#f2f2f2</Color>
    <Color x:Key="BlueGray">#e0e6f0</Color>
    <Color x:Key="FreshGreen">#7FFF00</Color>
    <Color x:Key="DarkFreshGreen">#4CAF50</Color>
    <Color x:Key="WaterBlue">#82B1FF</Color>
    <Color x:Key="DarkWaterBlue">#1976D2</Color>
    <Color x:Key="PopupDarkTransparent">#80000000</Color>
    <Color x:Key="TransparentWhite">#19FFFFFF</Color>

    <!-- Brushes -->
    <SolidColorBrush x:Key="TransparentWhiteBrush" Color="{StaticResource TransparentWhite}"/>
    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource Primary}"/>
    <SolidColorBrush x:Key="SecondaryBrush" Color="{StaticResource Secondary}"/>
    <SolidColorBrush x:Key="TertiaryBrush" Color="{StaticResource Tertiary}"/>
    <SolidColorBrush x:Key="WhiteBrush" Color="{StaticResource White}"/>
    <SolidColorBrush x:Key="BlackBrush" Color="{StaticResource Black}"/>
    <SolidColorBrush x:Key="Gray100Brush" Color="{StaticResource Gray100}"/>
    <SolidColorBrush x:Key="Gray200Brush" Color="{StaticResource Gray200}"/>
    <SolidColorBrush x:Key="Gray300Brush" Color="{StaticResource Gray300}"/>
    <SolidColorBrush x:Key="Gray400Brush" Color="{StaticResource Gray400}"/>
    <SolidColorBrush x:Key="Gray500Brush" Color="{StaticResource Gray500}"/>
    <SolidColorBrush x:Key="Gray600Brush" Color="{StaticResource Gray600}"/>
    <SolidColorBrush x:Key="Gray900Brush" Color="{StaticResource Gray900}"/>
    <SolidColorBrush x:Key="Gray950Brush" Color="{StaticResource Gray950}"/>
    <SolidColorBrush x:Key="Yellow100AccentBrush" Color="{StaticResource Yellow100Accent}"/>
    <SolidColorBrush x:Key="Yellow200AccentBrush" Color="{StaticResource Yellow200Accent}"/>
    <SolidColorBrush x:Key="Yellow300AccentBrush" Color="{StaticResource Yellow300Accent}"/>
    <SolidColorBrush x:Key="Cyan100AccentBrush" Color="{StaticResource Cyan100Accent}"/>
    <SolidColorBrush x:Key="Cyan200AccentBrush" Color="{StaticResource Cyan200Accent}"/>
    <SolidColorBrush x:Key="Cyan300AccentBrush" Color="{StaticResource Cyan300Accent}"/>
    <SolidColorBrush x:Key="Blue100AccentBrush" Color="{StaticResource Blue100Accent}"/>
    <SolidColorBrush x:Key="Blue200AccentBrush" Color="{StaticResource Blue200Accent}"/>
    <SolidColorBrush x:Key="Blue300AccentBrush" Color="{StaticResource Blue300Accent}"/>
    <SolidColorBrush x:Key="GridBackgroundBrush" Color="{StaticResource GridBackground}"/>
    <SolidColorBrush x:Key="NexenColorBrush" Color="{StaticResource NexenColor}"/>
    <SolidColorBrush x:Key="SomaDarkDarkBrush" Color="{StaticResource SomaDarkDark}"/>
    <SolidColorBrush x:Key="SomaDarkLightBrush" Color="{StaticResource SomaDarkLight}"/>
    <SolidColorBrush x:Key="GithubDarkBrush" Color="{StaticResource GithubDark}"/>
    <SolidColorBrush x:Key="GithubLightBrush" Color="{StaticResource GithubLight}"/>
    <SolidColorBrush x:Key="GithubRedBrush" Color="{StaticResource GithubRed}"/>
    <SolidColorBrush x:Key="GithubGreenBrush" Color="{StaticResource GithubGreen}"/>
    <SolidColorBrush x:Key="BootstrapWarningBgBrush" Color="{StaticResource BootstrapWarningBg}"/>
    <SolidColorBrush x:Key="LazyColorBrush" Color="{StaticResource LazyColor}"/>
    <SolidColorBrush x:Key="GrayBackgroundBrush" Color="{StaticResource GrayBackground}"/>
    <SolidColorBrush x:Key="BlueGrayBrush" Color="{StaticResource BlueGray}"/>
    <SolidColorBrush x:Key="FreshGreenBrush" Color="{StaticResource FreshGreen}"/>
    <SolidColorBrush x:Key="DarkFreshGreenBrush" Color="{StaticResource DarkFreshGreen}"/>
    <SolidColorBrush x:Key="WaterBlueBrush" Color="{StaticResource WaterBlue}"/>
    <SolidColorBrush x:Key="DarkWaterBlueBrush" Color="{StaticResource DarkWaterBlue}"/>
    <SolidColorBrush x:Key="PopupDarkTransparentBrush" Color="{StaticResource PopupDarkTransparent}"/>
    
</ResourceDictionary>
